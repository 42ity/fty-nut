[Unit]
Description=fty-nut-configuration service
After=network.target
# Requires=network.target
# Conflicts=shutdown.target
# PartOf=fty-nut.target

[Service]
Type=simple
# User=@uid@
Environment="prefix=@prefix@"
Environment='SYSTEMD_UNIT_FULLNAME=%n'
EnvironmentFile=/etc/default/bios-db-rw
ExecStart=@prefix@/bin/fty-nut-configuration -c @sysconfdir@/@PACKAGE@/fty-nut-configuration.cfg
Restart=always

[Install]
WantedBy=multi-user.target
# WantedBy=fty-nut.target
