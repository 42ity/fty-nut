[Unit]
Description=fty-nut-command service
Requires=network.target malamute.service
After=network.target
PartOf=bios.target

[Service]
Type=simple
User=bios
Restart=always
Environment="prefix=@prefix@"
Environment='SYSTEMD_UNIT_FULLNAME=%n'
EnvironmentFile=-/etc/default/bios-nut-admin
EnvironmentFile=-/etc/default/bios-db-ro
ExecStart=@prefix@/bin/fty-nut-command -c @sysconfdir@/@PACKAGE@/fty-nut-command.cfg

[Install]
WantedBy=bios.target
